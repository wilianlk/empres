{% extends "@App/Layouts/layout.html.twig" %}
{% block content %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-lg-10 box-form">
                    <div class="container"
                         style="width: 99%; background:#01374c; border-radius: 5px;margin-top: 5px;height:35px;">
                        <b style="color:white">Informacion de la Empresa</b>
                    </div>
                    <br>

                    {{ form_start(form, {'enctype': 'multipart/form-data', 'action': 'myaction', 'attr': {'class': 'form-inline'}}) }}
                    <div class="form-group">
                        {{ form_label(form.identificacion) }}
                        {{ form_widget(form.identificacion) }}
                    </div>
                    <div class="form-group">
                        {{ form_label(form.nombre) }}
                        {{ form_widget(form.nombre) }}
                    </div>
                    <div class="form-group">
                        {{ form_label(form.apellido) }}
                        {{ form_widget(form.apellido) }}
                    </div>
                    {{ form_end(form) }}


                    <button type="submit" class="btn btn-default">Register</button>

                    <div class="container"
                         style="width: 99%; background:#01374c; border-radius: 50px;margin-top: 5px;height:35px;">
                        <b style="color:white">Informacion de la Empresa</b>
                    </div>


                </div>


            </div>
        </div>
    </div>


{% endblock %}
